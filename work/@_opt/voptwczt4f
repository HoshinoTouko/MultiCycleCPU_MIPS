library verilog;
use verilog.vl_types.all;
entity dm is
    port(
        clk             : in     vl_logic;
        addr            : in     vl_logic_vector(11 downto 2);
        BE              : in     vl_logic_vector(3 downto 0);
        din             : in     vl_logic_vector(31 downto 0);
        DMWr            : in     vl_logic;
        MemReadSigned   : in     vl_logic;
        dout            : out    vl_logic_vector(31 downto 0)
    );
end dm;
